<launch>
	<node pkg="stereo_webcam" type="stereo_node" name="driver">
		<param name="cameras" value="2"/>
		<param name="device0" value="/dev/video_left"/>
		<param name="device1" value="/dev/video_right"/>
		<param name="params0" value="file:///home/mckoval/calibration_320x240_left.yaml"/>
		<param name="params1" value="file:///home/mckoval/calibration_320x240_right.yaml"/>

		<param name="width"  value="320"/>
		<param name="height" value="240"/>
		<param name="nbuf"   value="10"/>
		<param name="fps"    value="10"/>

		<remap from="camera0" to="left"/>
		<remap from="camera1" to="right"/>
		<remap from="left/image" to="left/image_raw"/>
		<remap from="right/image" to="right/image_raw"/>
	</node>
</launch>
